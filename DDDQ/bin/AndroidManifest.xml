            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="@string/title_activity_second" >
        </activity>
        <activity
            android:name=".ImgViewActivity"
            android:label="@string/title_activity_img_view" >
        </activity>
        <activity
            android:name=".LayoutActivity"
            android:label="@string/title_activity_layout" >
        </activity>
        <activity
            android:name=".RelativeActivity"
            android:label="@string/title_activity_relative" >
        </activity>
        <activity
            android:name=".FrameActivity"
            android:label="@string/title_activity_frame" >
        </activity>
        <activity
            android:name=".AbsoluteActivity"
            android:label="@string/title_activity_absolute" >
        </activity>
        <activity
            android:name=".TableActivity"
            android:label="@string/title_activity_table" >
        </activity>
        <activity
            android:name=".ToastActivity"
            android:label="@string/title_activity_toast" >
        </activity>
        <activity
            android:name=".BackValueActivity"
            android:label="@string/title_activity_back_value" >
        </activity>
        <activity
            android:name=".ListViewActivity"
            android:label="@string/title_activity_list_view" >
        </activity>
        <activity
            android:name=".SimpleCurActivity"
            android:label="@string/title_activity_simple_cur" >
        </activity>
        <activity
            android:name=".SimpleAdActivity"
            android:label="@string/title_activity_simple_ad" >
        </activity>
    </application>

</manifest>
